" Vimperator Molokai
" Based on molokai.vim in https://github.com/vehk/dotfiles/blob/master/vim/colors/molokai.vim
" See also term colors: https://github.com/vehk/dotfiles/blob/master/colors/molokai
" ~ Wolfgang Mueller
hi Normal               background: #2c3e50; color:#fff;
hi CmdLine              background: #2c3e50; color:#fff;
hi CompGroup            background: #2c3e50; color:#fff;
hi CompTitle            background: #2c3e50; font-weight: bold;
hi CompTitle>*          color: #fff; padding: 1px 0.5ex;
hi CompItem             background:#2c3e50; color:#fff;
hi CompItem[selected]   color: #FF9926; background: #262426;
hi CompDesc             width: 500px; max-width: 500px; color: #465457;
hi CompMsg              margin-left: 16px;
hi ErrorMsg             background: #D7005F; color: #fff; font-weight: bold;
hi Filter               color: #FF8700; background: #262426;
hi StatusLine           color: #fff; background: transparent; font-weight: normal;
hi Hint                 font-family: Helvatica Neue; font-size: 9px; font-weight: bold; color: white; background-color: #2c3e50; border-color: ButtonShadow; border-width: 0px; border-style: solid; padding: 0px 1px 0px 1px;
hi Hint::after          content: attr(number);
hi HintActive           background-color: #88FF00; color: black;
hi HintElem             background-color: yellow; color: black;
hi HintImage            opacity: .5;
hi URL                  text-decoration: none; color: #87FF00;
hi ModeMsg              color: white; background: #3E6700; border-radius: 1px; padding: 0px 5px;
hi PromptText           color: white; background: #860EFF;
hi MoreMsg              background-color: #2c3e50; color: #87FF00;
hi Tag                  color: #005F87;
hi InfoMsg              background: #2c3e50; color: #87FF00;
hi WarningMsg           background: #2c3e50; color: #D7005F;
hi Null                 color: #005F87;
hi Number               color: #005F87;
hi NonText              color: #B973FF; min-height: 16px; padding-left: 2px;
hi Title                background-color: #D7005F; color: #fff; font-weight: bold;
hi Indicator            color: #D7005F;
hi ContentSeparator     border-top: 1px solid #262426; display: -moz-box;

